<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddPaymentController">
    <content>
        <GridPane hgap="10.0" vgap="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
         <children>
            <Label text="Mokėjimo data:" GridPane.rowIndex="1" />
            <Label text="Mokėjimo suma, EUR:" GridPane.rowIndex="2" />
            <DatePicker fx:id="payedDate" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="payedAmount" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <DatePicker fx:id="endDate" GridPane.columnIndex="1" />
            <Label text="Galiojimo data:" />
         </children></GridPane>
    </content>
</DialogPane>
